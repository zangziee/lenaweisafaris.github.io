/* 
  Lenawei Safaris - Ultra-Detailed, Modern CSS
  Author: Lenawei Safaris
  Description: World-class, visually stunning, responsive, and accessible stylesheet for a safari/travel single-page website.
*/

/* ===================== GLOBAL RESETS ===================== */
html, body {
  margin: 0;
  padding: 0;
  height: 100%;
  min-height: 100vh;
  font-family: 'Montserrat', Arial, sans-serif;
  background: #f9f6ef;
  color: #222;
  scroll-behavior: smooth;
}

*, *:before, *:after {
  box-sizing: border-box;
}

body {
  line-height: 1.7;
  font-size: 1.08rem;
  letter-spacing: .02em;
  background: linear-gradient(180deg, #f9f6ef 0%, #fffbe6 100%);
  min-height: 100vh;
}

/* ========== TYPOGRAPHY ========== */
h1, h2, h3, h4, h5, h6 {
  font-family: 'Merriweather', serif;
  font-weight: 700;
  margin-top: 0;
  color: #3a3a1a;
}

h1 { font-size: 2.8rem; margin: 1.2rem 0; }
h2 { font-size: 2.1rem; margin: 1rem 0; }
h3 { font-size: 1.35rem; margin: 1rem 0 .7rem 0; }
h4 { font-size: 1.15rem; }
p, ul, ol { margin-bottom: 1.2em; }

a {
  color: #448C2C;
  text-decoration: none;
  transition: color 0.2s;
}
a:hover, a:focus { color: #638e3b; text-decoration: underline; }

/* ========== CONTAINER & GRID ========== */
.container {
  width: 94%;
  max-width: 1200px;
  margin: 0 auto;
  position: relative;
  z-index: 1;
}
@media (max-width: 700px) {
  .container { width: 99%; }
}

/* ===================== HEADER & NAV ===================== */
header {
  position: sticky;
  top: 0;
  z-index: 100;
  background: rgba(255,255,255,0.97);
  box-shadow: 0 2px 18px rgba(188,176,96,0.08);
}
.header-bar {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: .7rem 0;
}
.logo-wrap {
  display: flex;
  align-items: center;
  gap: 1rem;
}
.logo {
  height: 54px;
  width: auto;
  border-radius: 8px;
  box-shadow: 0 2px 8px rgba(150,120,32,0.13);
}
.brand {
  font-size: 1.6rem;
  font-family: 'Merriweather', serif;
  letter-spacing: .03em;
  color: #35502a;
  font-weight: 700;
}
.brand-accent { color: #e6b800; }

/* NAVIGATION */
nav {
  flex: 1 1 auto;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.nav-links {
  list-style: none;
  display: flex;
  gap: 1.1rem;
  margin: 0;
  padding: 0;
  align-items: center;
}
.nav-link {
  font-family: 'Montserrat', Arial, sans-serif;
  font-size: 1.02rem;
  padding: .5rem 1.1rem;
  border-radius: 30px;
  transition: background 0.2s, color 0.2s;
  color: #4b5320;
  font-weight: 500;
  position: relative;
}
.nav-link.active,
.nav-link:hover,
.nav-link:focus {
  background: #e6b8001a;
  color: #35502a;
}
.nav-link.special {
  color: #fff;
  background: #e6b800;
  margin-left: .4rem;
  font-weight: bold;
}
.nav-link.special:hover { color: #f7ffe4; background: #bfa200; }
.nav-toggle {
  display: none;
  background: none;
  border: none;
  font-size: 1.7rem;
  margin-left: .7rem;
  color: #4b5320;
  cursor: pointer;
}
.header-cta .btn-highlight {
  background: #e6b800;
  color: #fff;
  padding: .75em 1.5em;
  border-radius: 28px;
  font-size: 1rem;
  font-weight: 700;
  transition: background .22s;
  border: none;
  box-shadow: 0 3px 12px rgba(230,184,0,0.13);
}
.header-cta .btn-highlight:hover { background: #bfa200; color: #fff; }

/* Responsive Nav */
@media (max-width: 950px) {
  .nav-links {
    flex-direction: column;
    background: #fffbe6;
    position: absolute;
    top: 70px; right: 0; left: 0;
    display: none;
    gap: 0;
    box-shadow: 0 4px 24px rgba(188,176,96,0.13);
    z-index: 999;
    padding: 1.2em 0;
  }
  .nav-links.open { display: flex; }
  .nav-link { padding: 1em 2em; border-radius: 0; }
  .nav-toggle { display: block; }
}

@media (max-width: 700px) {
  .header-bar { flex-direction: column; gap: .5rem; }
  .logo { height: 36px; }
}

/* ===================== HERO SECTION ===================== */
.hero {
  position: relative;
  min-height: 82vh;
  background: linear-gradient(rgba(31, 49, 24,.25),rgba(31,49,24,.10)), url('images/safari-hero.jpg') center/cover no-repeat;
  display: flex;
  align-items: center;
  color: #fff;
  box-shadow: 0 16px 32px rgba(88,88,25,0.11);
  overflow: hidden;
}
.hero-bg-overlay {
  position: absolute;
  inset: 0;
  background: linear-gradient(270deg, rgba(230,184,0,0.10) 0%, rgba(31,49,24,0.34) 100%);
  z-index: 1;
}
.hero-content {
  position: relative;
  z-index: 2;
  display: flex;
  align-items: flex-start;
  justify-content: space-between;
  width: 100%;
  gap: 3vw;
  padding: 4vw 2vw 2vw 2vw;
}
.hero-text {
  flex: 2 1 450px;
  max-width: 620px;
  color: #fff;
  text-shadow: 0 3px 16px rgba(31,49,24,0.17);
}
.hero-text h1 {
  font-size: 2.8rem;
  line-height: 1.1;
  margin-bottom: .6em;
  color: #fff;
}
.hero-text .highlight {
  color: #e6b800;
  background: rgba(31,49,24,0.16);
  padding: 0 .27em;
  border-radius: 8px;
}
.hero-text p {
  font-size: 1.2rem;
  margin-bottom: 1.4em;
  color: #f7ffe4;
}
.hero-btns {
  display: flex;
  gap: 1.2em;
  margin-bottom: 1.2em;
}
.btn, .btn-lg, .btn-alt, .btn-sm {
  display: inline-block;
  text-align: center;
  border: none;
  border-radius: 32px;
  padding: .7em 2em;
  background: #4b5320;
  color: #ffe082;
  font-weight: 700;
  font-size: 1.08rem;
  cursor: pointer;
  margin-right: .7em;
  transition: background .22s, color .22s, box-shadow .22s;
  box-shadow: 0 4px 18px rgba(76, 83, 32, 0.12);
}
.btn-lg { font-size: 1.18rem; padding: 1em 2.5em; }
.btn-alt {
  background: #e6b800;
  color: #fff;
}
.btn-alt:hover, .btn-lg:hover, .btn:hover {
  background: #638e3b;
  color: #fff;
}
.btn-sm {
  font-size: .99rem;
  padding: .4em 1.2em;
  background: #fffbe6;
  color: #4b5320;
  border: 1.5px solid #e6b800;
  margin-top: 1em;
}
.btn-sm:hover { background: #e6b800; color: #fff; }
.hero-awards img {
  height: 38px;
  margin-right: 1em;
  background: #fffbe6;
  border-radius: 8px;
  box-shadow: 0 1px 7px rgba(136,100,0,0.13);
}
.hero-media {
  flex: 1 1 350px;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  gap: 1.5rem;
}
.hero-img {
  width: 320px;
  max-width: 100%;
  border-radius: 16px;
  box-shadow: 0 8px 32px rgba(31,49,24,0.23);
  margin-bottom: .6em;
}
.hero-video {
  width: 320px;
  max-width: 100%;
  aspect-ratio: 16/9;
  border-radius: 14px;
  overflow: hidden;
  box-shadow: 0 8px 32px rgba(31,49,24,0.13);
  background: #222;
}
.hero-video iframe {
  width: 100%;
  height: 100%;
  border: none;
}
.hero-socials {
  position: absolute;
  bottom: 40px;
  left: 40px;
  display: flex;
  gap: 1.3em;
  z-index: 3;
}
.hero-socials a {
  font-size: 1.44rem;
  color: #fffbe6;
  background: #35502a;
  border-radius: 50%;
  width: 38px; height: 38px;
  display: flex;
  align-items: center; justify-content: center;
  transition: background .21s, color .21s;
}
.hero-socials a:hover { background: #e6b800; color: #35502a; }
.scroll-down-indicator {
  position: absolute;
  bottom: 18px;
  left: 50%;
  transform: translateX(-50%);
  z-index: 4;
}
.scroll-down-indicator a {
  color: #e6b800;
  font-size: 2.2rem;
  animation: bounce 2s infinite;
}
@keyframes bounce {
  0%,100% { transform: translateY(0); }
  50% { transform: translateY(10px);}
}

/* Responsive Hero */
@media (max-width: 1000px) {
  .hero-content { flex-direction: column; align-items: flex-start; }
  .hero-media { align-items: flex-start; }
  .hero-socials { left: 20px; bottom: 16px; }
}
@media (max-width: 700px) {
  .hero-content { padding: 3vw 1vw 1vw 1vw; }
  .hero-img, .hero-video { width: 98vw; }
}

/* ===================== WHY CHOOSE US ===================== */
.why-choose { background: #fffbe6; padding: 3rem 0; }
.why-choose h2 { text-align: center; color: #4b5320; }
.why-grid {
  display: flex;
  flex-wrap: wrap;
  gap: 2rem;
  margin-top: 2.5rem;
  justify-content: center;
}
.why-card {
  flex: 1 1 230px;
  max-width: 320px;
  background: #f9f6ef;
  border-radius: 16px;
  padding: 1.8rem 1.2rem;
  box-shadow: 0 4px 24px rgba(136,100,0,0.06);
  text-align: center;
  transition: box-shadow .17s, transform .17s;
}
.why-card:hover {
  box-shadow: 0 8px 32px rgba(136,100,0,0.14);
  transform: translateY(-7px) scale(1.04);
}
.why-card i {
  font-size: 2.3rem;
  color: #e6b800;
  margin-bottom: .7em;
}
.why-card h3 {
  color: #35502a;
  font-size: 1.23rem;
  margin-bottom: .4em;
}
.why-card p { color: #555; font-size: 1.01rem; }

/* ===================== DESTINATIONS GRID ===================== */
.destinations { background: #fff; padding: 4rem 0 3rem 0; }
.destinations h2, .destinations .subtitle { text-align: center; }
.destinations-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(230px, 1fr));
  gap: 2.2rem;
  margin-top: 2.5rem;
}
.destination-card {
  background: #f9f6ef;
  border-radius: 16px;
  box-shadow: 0 2px 16px rgba(188,176,96,0.11);
  overflow: hidden;
  display: flex;
  flex-direction: column;
  align-items: center;
  transition: box-shadow .19s, transform .18s;
  position: relative;
}
.destination-card:hover {
  box-shadow: 0 8px 32px rgba(136,100,0,0.18);
  transform: translateY(-4px) scale(1.03);
}
.destination-card img {
  width: 100%; height: 120px; object-fit: cover;
}
.destination-card h3 {
  margin: 1rem 0 .5rem 0;
  color: #e6b800;
}
.destination-card ul {
  list-style: disc inside;
  padding: 0 1.3rem;
  margin: 0 0 1.2rem 0;
  color: #444;
}
.destination-card .btn-sm {
  margin-bottom: 1.2rem;
  background: #e6b800;
  color: #fff;
}
.destination-card .btn-sm:hover { background: #35502a; }

/* ===================== TOURS ===================== */
.tours { background: #fffbe6; padding: 3.5rem 0; }
.tours h2, .tours .subtitle { text-align: center; }
.tour-cards {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(340px, 1fr));
  gap: 2.5rem;
  margin-top: 2.5rem;
}
.tour-card {
  background: #fff;
  border-radius: 16px;
  box-shadow: 0 4px 22px rgba(188,176,96,0.13);
  overflow: hidden;
  display: flex;
  flex-direction: column;
  transition: box-shadow .18s, transform .16s;
  border: 2px solid #f9f6ef;
}
.tour-card:hover {
  box-shadow: 0 12px 36px rgba(136,100,0,0.17);
  transform: translateY(-8px) scale(1.03);
  border-color: #e6b80033;
}
.tour-card img {
  width: 100%; height: 170px; object-fit: cover;
}
.tour-info {
  padding: 1.1em 1.3em;
  display: flex; flex-direction: column;
  flex: 1 1 auto;
}
.tour-info h3 { font-size: 1.23rem; color: #e6b800; margin: 0.6em 0 0.3em 0; }
.tour-meta { font-size: .99rem; color: #35502a; margin-bottom: .7em; }
.tour-tags {
  margin-bottom: .6em;
  display: flex; flex-wrap: wrap; gap: .5em;
}
.tag {
  display: inline-block;
  background: #f4e8bb;
  color: #4b5320;
  border-radius: 24px;
  padding: .18em .85em;
  font-size: .88em;
  font-weight: 600;
  margin-right: .2em;
  border: 1.2px solid #e6b800;
}
.tag.luxury { background: #e6b800; color: #fff; }
.tag.best { background: #4b5320; color: #fff; }
.tag.family { background: #81a14b; color: #fff; }
.tag.water { background: #4b90b3; color: #fff; }
.tag.adventure { background: #e69500; color: #fff; }
.tag.primates { background: #b05923; color: #fff; }
.tag.wildlife { background: #638e3b; color: #fff; }
.tag.desert { background: #e6b800; color: #fff; }
.tag.exclusive { background: #b05923; color: #fff; }
.tag.active { background: #5da94d; color: #fff; }
.tour-info ul { margin: 0 0 .6em 1em; padding: 0; color: #444; }
.tour-info .btn-sm { margin-top: .5em; }
.tours-cta {
  text-align: center;
  margin: 2.6em 0 1.4em 0;
}
.special-offers {
  background: #fff9e3;
  border-radius: 13px;
  margin: 2.4em auto 0 auto;
  padding: 1.1em 2em;
  max-width: 660px;
  box-shadow: 0 1px 7px rgba(230,184,0,0.08);
}
.special-offers h3 { color: #e6b800; }
.special-offers ul { color: #444; }

/* ===================== GALLERY ===================== */
.gallery { background: #fff; padding: 3.8rem 0; }
.gallery h2, .gallery .subtitle { text-align: center; }
.gallery-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(190px, 1fr));
  gap: 1.3rem;
  margin-top: 2.1rem;
  margin-bottom: 2.2rem;
}
.gallery-item {
  overflow: hidden;
  border-radius: 12px;
  box-shadow: 0 1px 7px rgba(188,176,96,0.07);
  background: #fffbe6;
  position: relative;
  transition: box-shadow .18s, transform .15s;
  cursor: pointer;
}
.gallery-item img {
  width: 100%; display: block;
  height: 160px; object-fit: cover;
  transition: transform 0.22s;
}
.gallery-item span {
  display: block;
  text-align: center;
  color: #4b5320;
  background: #e6b800cc;
  font-weight: bold;
  padding: .45em 0;
  position: absolute; bottom: 0; left: 0; width: 100%;
  font-size: 1rem;
  text-shadow: 0 2px 8px #2222;
}
.gallery-item:hover, .gallery-item:focus {
  box-shadow: 0 8px 32px rgba(136,100,0,0.15);
  transform: scale(1.04);
}
.gallery-item:hover img { transform: scale(1.07); }

.gallery-video {
  margin: 2.6em auto 0 auto;
  max-width: 720px;
  aspect-ratio: 16/9;
  border-radius: 14px;
  overflow: hidden;
  box-shadow: 0 8px 32px rgba(31,49,24,0.13);
  background: #222;
}
.gallery-video iframe {
  width: 100%; height: 100%; border: none;
}

/* Lightbox Modal for Gallery (hidden by default) */
.modal {
  display: none;
  position: fixed;
  z-index: 99;
  left: 0; top: 0; width: 100vw; height: 100vh;
  background: rgba(31,49,24,0.90);
  align-items: center; justify-content: center;
  flex-direction: column;
}
.modal.open { display: flex; }
.modal-img { max-width: 90vw; max-height: 72vh; border-radius: 16px; box-shadow: 0 6px 32px #222d; }
.modal-close {
  position: absolute; top: 2.8rem; right: 4vw;
  font-size: 2.6rem; color: #fffbe6; cursor: pointer;
}
.modal-caption { color: #fffbe6; margin-top: 1em; font-size: 1.12em; }
.modal-prev, .modal-next {
  position: absolute; top: 50%;
  transform: translateY(-50%);
  font-size: 2.3rem; color: #e6b800;
  background: none; border: none; cursor: pointer;
  padding: .4em .7em;
  border-radius: 50%;
  opacity: 0.70;
  z-index: 101;
}
.modal-prev { left: 2vw; }
.modal-next { right: 2vw; }
.modal-prev:hover, .modal-next:hover { background: #e6b80044; opacity: 1; }

/* ===================== ABOUT US ===================== */
.about { background: #fffbe6; padding: 4.2rem 0; }
.about-flex {
  display: flex;
  gap: 3em;
  align-items: flex-start;
}
.about-main, .about-imgs {
  flex: 1 1 350px;
}
.about-main { max-width: 530px; }
.about-imgs { display: flex; flex-direction: column; gap: 1.4em; }
.about-imgs img {
  width: 100%; border-radius: 16px;
  box-shadow: 0 2px 12px rgba(136,100,0,0.13);
}
.about-history { margin-top: 2em; }
.about-history ol { padding-left: 1.2em; color: #444; }
.about-video {
  margin: 2.2em auto 0 auto;
  max-width: 600px;
  aspect-ratio: 16/9;
  border-radius: 14px;
  overflow: hidden;
  box-shadow: 0 8px 32px rgba(31,49,24,0.13);
  background: #222;
}
.about-video iframe { width: 100%; height: 100%; border: none; }

/* ===================== IMPACT & SUSTAINABILITY ===================== */
.impact { background: #fff; padding: 4rem 0; }
.impact-flex {
  display: flex;
  gap: 2.5em;
  justify-content: center;
  flex-wrap: wrap;
}
.impact-card {
  background: #f9f6ef;
  border-radius: 15px;
  box-shadow: 0 2px 16px rgba(136,100,0,0.09);
  padding: 1.6em 1.2em;
  width: 220px; max-width: 90vw;
  text-align: center;
}
.impact-card i {
  font-size: 2.2rem;
  color: #e6b800;
  margin-bottom: .4em;
}
.impact-card h3 { margin-bottom: .3em; color: #35502a; }
.impact-stats {
  margin-top: 2.7em;
  text-align: center;
}
.impact-stats ul {
  display: flex; flex-wrap: wrap; gap: 1.7em;
  justify-content: center;
  font-size: 1.08em;
  color: #222;
  margin: 0;
}
.impact-stats li {
  background: #f7ffe4;
  padding: .8em 1.6em;
  border-radius: 9px;
  box-shadow: 0 1px 6px #e6b80022;
  margin-bottom: .5em;
}

/* ===================== TEAM & GUIDES ===================== */
.team { background: #fff; padding: 4rem 0; }
.team-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(200px,1fr));
  gap: 2.3em;
  margin-top: 2.5em;
}
.team-member {
  background: #f9f6ef;
  border-radius: 14px;
  box-shadow: 0 2px 12px rgba(136,100,0,0.08);
  text-align: center;
  padding: 1.2em .9em;
  transition: box-shadow .19s, transform .15s;
}
.team-member:hover {
  box-shadow: 0 8px 32px rgba(136,100,0,0.13);
  transform: scale(1.04);
}
.team-member img {
  width: 80px; height: 80px;
  border-radius: 50%;
  object-fit: cover;
  margin-bottom: .6em;
  border: 3px solid #e6b80033;
}
.team-member h3 { color: #4b5320; margin-bottom: .2em; }
.team-member p { color: #555; margin-bottom: .7em; }
.team-member ul {
  text-align: left;
  font-size: .98em;
  color: #444;
  margin: 0 auto;
  display: inline-block;
}

/* ===================== TESTIMONIALS ===================== */
.testimonials { background: #fffbe6; padding: 4rem 0; }
.testimonials-slider {
  display: flex;
  overflow-x: auto;
  gap: 2.5em;
  margin-top: 2.5em;
  scroll-snap-type: x mandatory;
}
.testimonial {
  min-width: 330px;
  background: #f9f6ef;
  border-radius: 13px;
  box-shadow: 0 2px 16px rgba(136,100,0,0.11);
  padding: 2em 1.2em 1.5em 1.2em;
  text-align: left;
  position: relative;
  scroll-snap-align: start;
}
.testimonial:before {
  content: "“"; color: #e6b800; font-size: 2.2rem; position: absolute; left: 1em; top: .3em;
}
.testimonial p {
  font-style: italic;
  color: #3a3a1a;
  margin: 0 0 .7em 0;
}
.testimonial footer {
  display: flex; align-items: center; gap: .7em;
  margin-top: 1.1em; color: #638e3b;
  font-size: .93em;
}
.testimonial footer img {
  width: 36px; height: 36px; border-radius: 50%; object-fit: cover;
  border: 2px solid #e6b80044;
}
.testimonial-nav {
  display: flex; justify-content: center; gap: 1.2em;
  margin-top: 2em;
}
.testimonial-prev, .testimonial-next {
  background: #e6b800;
  color: #fff;
  border: none;
  border-radius: 50%;
  width: 44px; height: 44px;
  font-size: 1.6rem;
  cursor: pointer;
  transition: background .18s;
}
.testimonial-prev:hover, .testimonial-next:hover { background: #35502a; }

@media (max-width: 800px) {
  .testimonials-slider { flex-direction: column; gap: 1.7em; }
  .testimonial { min-width: 0; }
}

/* ===================== FAQ ===================== */
.faq { background: #fff; padding: 4rem 0; }
.faq-list {
  max-width: 700px;
  margin: 2.4em auto 0 auto;
}
.faq-item {
  background: #fffbe6;
  border-radius: 10px;
  box-shadow: 0 2px 10px #e6b80017;
  margin-bottom: 1.5em;
  overflow: hidden;
  transition: box-shadow .14s;
}
.faq-question {
  width: 100%;
  background: none;
  border: none;
  padding: 1em 1.2em;
  font-size: 1.1rem;
  color: #35502a;
  font-weight: bold;
  text-align: left;
  cursor: pointer;
  transition: background .13s;
  position: relative;
}
.faq-question:after {
  content: '\f078';
  font-family: 'Font Awesome 6 Free';
  font-weight: 900;
  position: absolute;
  right: 1.2em; top: 1.2em;
  font-size: 1em;
  color: #e6b800;
  transition: transform .21s;
}
.faq-question[aria-expanded="true"]:after {
  transform: rotate(180deg);
}
.faq-item .faq-answer {
  max-height: 0;
  overflow: hidden;
  background: #f9f6ef;
  color: #333;
  font-size: 1.02em;
  transition: max-height .29s cubic-bezier(.4,0,.2,1);
  padding: 0 1.2em;
}
.faq-item.open .faq-answer {
  max-height: 200px;
  padding: 1em 1.2em;
}

/* ===================== BLOG/NEWS ===================== */
.blog { background: #fffbe6; padding: 4rem 0; }
.blog-cards {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
  gap: 2em;
  margin-top: 2.4em;
}
.blog-card {
  background: #fff;
  border-radius: 12px;
  box-shadow: 0 2px 12px rgba(136,100,0,0.09);
  overflow: hidden;
  display: flex;
  flex-direction: column;
  transition: box-shadow .16s, transform .15s;
}
.blog-card:hover {
  box-shadow: 0 8px 32px rgba(136,100,0,0.13);
  transform: scale(1.04);
}
.blog-card img {
  width: 100%; height: 135px; object-fit: cover;
}
.blog-card div { padding: 1.1em 1em; }
.blog-card h3 { font-size: 1.15rem; margin-bottom: .4em; }
.blog-meta { font-size: .92em; color: #555; }

/* ===================== RESOURCES ===================== */
.resources { background: #fff; padding: 4rem 0; }
.resources-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(230px,1fr));
  gap: 1.5em;
  margin-top: 2.2em;
}
.resource-card {
  background: #f9f6ef;
  border-radius: 13px;
  padding: 1.5em 1.2em;
  box-shadow: 0 2px 12px rgba(136,100,0,0.07);
  text-align: center;
}
.resource-card i {
  font-size: 2rem;
  color: #e6b800;
  margin-bottom: .5em;
}
.resource-card h3 { color: #35502a; margin-bottom: .3em; }
.resource-card ul { color: #444; }

/* ===================== NEWSLETTER ===================== */
.newsletter { background: #fffbe6; padding: 4rem 0; text-align: center; }
#newsletter-form {
  margin: 2em auto 0 auto;
  max-width: 400px;
  display: flex; flex-direction: column; gap: .8em;
  align-items: center;
}
#newsletter-form label { font-weight: 600; color: #35502a; }
#newsletter-email {
  width: 100%;
  border-radius: 22px;
  border: 1.5px solid #e6b800;
  padding: .8em 1.2em;
  font-size: 1.08rem;
  background: #fff;
}
#newsletter-form .btn {
  width: 100%;
  background: #e6b800;
  color: #fff;
  margin-top: .6em;
}
#newsletter-form .btn:hover { background: #35502a; }
#newsletter-msg {
  display: block;
  color: #35502a;
  font-weight: 600;
  margin-top: .6em;
}

/* ===================== PARTNERS & AWARDS ===================== */
.partners { background: #fff; padding: 3rem 0; }
.partners-logos {
  display: flex;
  flex-wrap: wrap;
  gap: 2.5em;
  justify-content: center;
  align-items: center;
  margin-top: 2em;
}
.partners-logos img {
  width: 120px;
  max-width: 24vw;
  background: #fffbe6;
  border-radius: 10px;
  box-shadow: 0 1px 7px #e6b80019;
  padding: .7em;
  margin-bottom: .7em;
}

/* ===================== OFFERS & DEALS ===================== */
.offers { background: #fffbe6; padding: 3rem 0; }
.offers-grid {
  display: flex; flex-wrap: wrap; gap: 2em; justify-content: center; margin-top: 2em;
}
.offer-card {
  background: #fff;
  border-radius: 10px;
  box-shadow: 0 2px 10px rgba(136,100,0,0.10);
  padding: 1.4em 1.2em;
  min-width: 210px; max-width: 290px;
  text-align: center;
}
.offer-card h3 { color: #e6b800; margin-bottom: .3em; }

/* ===================== CONTACT & BOOKING ===================== */
.contact { background: #fff; padding: 4rem 0; }
.contact-boxes {
  display: flex;
  gap: 2.2em;
  margin-top: 2.4em;
  flex-wrap: wrap;
  align-items: flex-start;
  justify-content: center;
}
.contact-form-box, .contact-details-box {
  flex: 1 1 340px;
  min-width: 320px;
  background: #f9f6ef;
  border-radius: 13px;
  box-shadow: 0 2px 10px #e6b80013;
  padding: 1.4em 1.2em;
}
.contact-form-box form label {
  font-weight: 600;
  color: #35502a;
  margin: .7em 0 .2em 0;
  display: block;
}
.contact-form-box input,
.contact-form-box select,
.contact-form-box textarea {
  width: 100%;
  border-radius: 8px;
  border: 1.4px solid #e6b800;
  padding: .7em 1em;
  margin-bottom: .5em;
  font-size: 1.04rem;
  background: #fff;
}
.contact-form-box textarea { min-height: 80px; }
.contact-form-box .btn {
  width: 100%;
  background: #e6b800;
  color: #fff;
  margin-top: .7em;
}
.contact-form-box .btn:hover { background: #35502a; }
.contact-details-box h3 { color: #4b5320; margin-bottom: .5em; }
.contact-details-box p, .contact-details-box a { color: #35502a; font-size: 1.06em; }
.contact-socials {
  display: flex; gap: 1em; margin-top: .8em; margin-bottom: .8em;
}
.contact-socials a {
  font-size: 1.4rem;
  color: #fffbe6;
  background: #35502a;
  border-radius: 50%;
  width: 32px; height: 32px;
  display: flex; align-items: center; justify-content: center;
  transition: background .21s, color .21s;
}
.contact-socials a:hover { background: #e6b800; color: #35502a; }
.contact-map {
  margin-top: 1.1em;
  border-radius: 9px;
  overflow: hidden;
  box-shadow: 0 1px 6px #e6b80022;
}

/* ===================== FOOTER ===================== */
footer {
  background: #4b5320;
  color: #ffe082;
  text-align: center;
  padding: 2.2rem 0 1.5rem 0;
  margin-top: 1.5rem;
  border-radius: 22px 22px 0 0;
  font-size: 1.05rem;
  position: relative;
}
footer nav {
  margin-bottom: .8em;
  display: flex; justify-content: center; gap: 1.5em;
}
footer nav a {
  color: #ffe082;
  font-weight: bold;
  text-decoration: underline;
  font-size: 1.05rem;
}
footer nav a:hover { color: #fffbe6; text-decoration: none; }
.footer-socials {
  margin-top: 1.1em;
  display: flex; justify-content: center; gap: 1em;
}
.footer-socials a {
  font-size: 1.3rem;
  color: #e6b800;
  background: #fffbe6;
  border-radius: 50%;
  width: 32px; height: 32px;
  display: flex; align-items: center; justify-content: center;
  transition: background .21s, color .21s;
}
.footer-socials a:hover { background: #e6b800; color: #4b5320; }

/* ===================== SCROLL TO TOP BUTTON ===================== */
#scroll-to-top {
  position: fixed;
  bottom: 32px; right: 32px;
  background: #e6b800;
  color: #fff;
  border: none;
  border-radius: 50%;
  width: 52px; height: 52px;
  font-size: 2.1rem;
  cursor: pointer;
  box-shadow: 0 2px 12px #e6b80022;
  z-index: 999;
  opacity: 0; pointer-events: none;
  transition: opacity .21s;
}
#scroll-to-top.show { opacity: 1; pointer-events: auto; }
#scroll-to-top:hover { background: #4b5320; color: #fffbe6; }

/* ===================== PRINT STYLES ===================== */
@media print {
  header, nav, .btn, .scroll-down-indicator, .hero-video, .gallery-video, #scroll-to-top, .testimonial-nav, .contact-socials, .footer-socials, .offers, .newsletter, .partners, .special-offers, .tours-cta {
    display: none !important;
  }
  body, .container {
    background: #fff !important;
    color: #111 !important;
  }
}

/* ===================== RESPONSIVE BREAKPOINTS ===================== */
@media (max-width: 1000px) {
  .about-flex, .impact-flex, .contact-boxes {
    flex-direction: column;
    gap: 1.6em;
    align-items: stretch;
  }
  .hero-content { flex-direction: column; align-items: flex-start; }
}
@media (max-width: 800px) {
  .tour-cards, .gallery-grid, .team-grid, .resources-grid, .destinations-grid, .blog-cards, .offers-grid {
    grid-template-columns: 1fr;
    gap: 1.2em;
  }
  .hero-media, .about-imgs { display: none; }
  .container { width: 99%; }
}
@media (max-width: 600px) {
  .hero-text h1 { font-size: 2.1rem; }
  h2 { font-size: 1.4rem; }
  .btn, .btn-lg, .btn-alt { padding: .6em 1.1em; font-size: .97rem; }
  .special-offers, .about-history, .about-video, .impact-stats, .gallery-video, .newsletter, .partners { padding: 1.3em .4em; }
  footer { font-size: .96rem; padding: 1.1rem 0 1rem 0; }
}

/* ===================== ACCESSIBILITY ===================== */
:focus {
  outline: 2.4px solid #e6b800;
  outline-offset: 2px;
}
[aria-live="polite"] { font-style: italic; color: #e6b800; }

/* ===================== CUSTOM SCROLLBAR ===================== */
::-webkit-scrollbar { width: 10px; background: #fffbe6; }
::-webkit-scrollbar-thumb {
  background: #e6b80077;
  border-radius: 6px;
}
::-webkit-scrollbar-thumb:hover { background: #e6b800cc; }
